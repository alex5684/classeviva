					app.errorMsg('CRD_ERR_PASSWORD_REMIND_RECAPTCHA_FAIL');
					$('.usr-remind-captcha-container').html('');
					$('.usr-remind-captcha-container').html('<div class="g-recaptcha" data-sitekey="6LchJPkUAAAAANAZrvwiloiDmeY2zBJWqZ_rOFJD"></div>');
