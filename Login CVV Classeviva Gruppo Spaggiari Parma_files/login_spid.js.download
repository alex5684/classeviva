				var app = this;
				var list = (l !== undefined) ? l : null;
				var error = {
					id: '',
					desc: ''
				};
				
				app.accedi_btn_clicked = false;
				app.invia_btn_clicked = false;
				
				if(app.LOGIN_ERRORS[err] !== undefined)
				{
					list = 'loginErrors';
